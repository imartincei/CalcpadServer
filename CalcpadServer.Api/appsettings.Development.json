{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "Provider": "Local",
    "Local": {
      "Enabled": true,
      "RequireEmailConfirmation": false,
      "AllowUserRegistration": true,
      "Jwt": {
        "Secret": "development-jwt-secret-key-at-least-32-characters-long",
        "ExpiryInHours": 8,
        "Issuer": "CalcpadServer-Dev",
        "Audience": "CalcpadClients-Dev"
      }
    }
  }
}
